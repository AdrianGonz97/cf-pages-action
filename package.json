{
	"name": "refined-cf-pages-action",
	"version": "1.0.1",
	"description": "Publish to Cloudflare Pages",
	"main": "dist/index.js",
	"scripts": {
		"build": "pnpm tsup",
		"dev": "pnpm tsup --watch",
		"prepare": "husky install",
		"format": "prettier . --write --ignore-unknown",
		"format:check": "prettier --check .",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"check": "pnpm tsc"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AdrianGonz97/refined-cf-pages-action.git"
	},
	"keywords": [
		"Cloudflare Pages",
		"GitHub Actions"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/AdrianGonz97/refined-cf-pages-action/issues"
	},
	"homepage": "https://github.com/AdrianGonz97/refined-cf-pages-action#readme",
	"devDependencies": {
		"@actions/core": "^1.10.0",
		"@actions/github": "^5.1.1",
		"@cloudflare/types": "^6.18.16",
		"@types/node": "^18.11.3",
		"@typescript-eslint/eslint-plugin": "^6.18.0",
		"@typescript-eslint/parser": "^6.18.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"husky": "^8.0.1",
		"prettier": "^3.1.1",
		"shellac": "^0.7.2",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3",
		"undici": "^5.11.0"
	}
}
